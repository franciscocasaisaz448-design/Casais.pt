--                        CONTROLE DO PAINEL
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function showPanel(show)
    if panelVisible == show then return end
    panelVisible = show

    if show then
        mainFrame.Visible = true
        TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back), {
            Size = UDim2.fromOffset(750, 500),
            Position = UDim2.new(0.5, -375, 0.5, -250)
        }):Play()

        -- AnimaÃ§Ã£o do conteÃºdo
        for _, child in ipairs(mainFrame:GetChildren()) do
            if child ~= header then
                child.Position = UDim2.new(child.Position.X.Scale, child.Position.X.Offset, 1.2, child.Position.Y.Offset)
                TweenService:Create(child, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
                    Position = UDim2.new(child.Position.X.Scale, child.Position.X.Offset, child.Position.Y.Scale, child.Position.Y.Offset)
                }):Play()
            end
        end
    else
        TweenService:Create(mainFrame, TweenInfo.new(0.3), {
            Size = UDim2.fromOffset(0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        }):Play()
        task.wait(0.3)
        mainFrame.Visible = false
    end
end

closeBtn.MouseButton1Click:Connect(function()
    showPanel(false)
end)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                        EVENTOS DE ENTRADA
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

UserInputService.InputBegan:Connect(function(input, processed)
    if processed then return end

    if input.KeyCode == Enum.KeyCode.M then
        showPanel(not panelVisible)
    elseif input.KeyCode == Enum.KeyCode.F and panelVisible then
        _G.FlySystem:toggleFly()
        flyToggle.Text = "FLY: " .. (_G.FlySystem.flying and "ON" or "OFF")
        flyToggle.BackgroundColor3 = _G.FlySystem.flying and Color3.fromRGB(0, 160, 90) or Color3.fromRGB(60, 60, 70)
    end
end)

