local function createCard(parent, title, description, color)
    local card = Instance.new("Frame")
    card.Size = UDim2.new(1, -20, 0, 120)
    card.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    card.Parent = parent
    Instance.new("UICorner", card).CornerRadius = UDim.new(0, 12)

    local cardStroke = Instance.new("UIStroke", card)
    cardStroke.Color = color
    cardStroke.Thickness = 2
    cardStroke.Transparency = 0.7

    local icon = Instance.new("TextLabel")
    icon.Size = UDim2.new(0, 40, 0, 40)
    icon.Position = UDim2.new(0, 15, 0, 15)
    icon.BackgroundColor3 = color
    icon.BackgroundTransparency = 0.8
    icon.Text = "âš¡"
    icon.Font = Enum.Font.GothamBold
    icon.TextSize = 18
    icon.TextColor3 = Color3.new(1,1,1)
    icon.Parent = card
    Instance.new("UICorner", icon).CornerRadius = UDim.new(0, 8)

    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -80, 0, 25)
    titleLabel.Position = UDim2.new(0, 65, 0, 15)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = title
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextSize = 16
    titleLabel.TextColor3 = Color3.new(1,1,1)
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = card

    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(1, -80, 0, 40)
    descLabel.Position = UDim2.new(0, 65, 0, 40)
    descLabel.BackgroundTransparency = 1
    descLabel.Text = description
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextSize = 11
    descLabel.TextColor3 = Color3.fromRGB(150,150,160)
    descLabel.TextWrapped = true
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.Parent = card

    return card, icon, titleLabel, descLabel
end

local function createToggle(card, text, callback)
    local toggle = Instance.new("TextButton")
    toggle.Size = UDim2.new(0, 100, 0, 35)
    toggle.Position = UDim2.new(1, -115, 0, 75)
    toggle.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
    toggle.Text = text .. ": OFF"
    toggle.Font = Enum.Font.GothamBold
    toggle.TextSize = 12
    toggle.TextColor3 = Color3.new(1,1,1)
    toggle.Parent = card
    Instance.new("UICorner", toggle).CornerRadius = UDim.new(0, 8)

    local enabled = false
    toggle.MouseButton1Click:Connect(function()
        enabled = not enabled
        toggle.Text = text .. ": " .. (enabled and "ON" or "OFF")
        toggle.BackgroundColor3 = enabled and Color3.fromRGB(0, 160, 90) or Color3.fromRGB(60, 60, 70)
        if callback then callback(enabled) end
    end)

    return toggle
end

