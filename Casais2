-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                      SISTEMA ESP MELHORADO
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local espEnabled = true
local espBoxEnabled = false
local espMaxDistance = 1000
local espHighlights = {}
local espBoxes = {}
local tracersEnabled = false
local tracerLines = {}
local nametagsEnabled = false
local nametagsGuis = {}

local function getPlayerDistance(player)
    if not LocalPlayer.Character or not player.Character then return math.huge end
    local myPos = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    local theirPos = player.Character:FindFirstChild("HumanoidRootPart")
    if not myPos or not theirPos then return math.huge end
    return (myPos.Position - theirPos.Position).Magnitude
end

local function createESPBox(player)
    if not Drawing then return end

    local box = {
        topLeft = Drawing.new("Line"),
        topRight = Drawing.new("Line"),
        bottomLeft = Drawing.new("Line"),
        bottomRight = Drawing.new("Line")
    }

    for _, line in pairs(box) do
        line.Thickness = 2
        line.Color = Color3.fromRGB(0, 255, 0)
        line.Transparency = 1
        line.Visible = false
    end

    return box
end

local function updateESPBox(player, box)
    if not player.Character or not box then return end

    local char = player.Character
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local head = char:FindFirstChild("Head")

    if not hrp or not head then
        for _, line in pairs(box) do
            line.Visible = false
        end
        return
    end

    local distance = getPlayerDistance(player)
    if distance > espMaxDistance then
        for _, line in pairs(box) do
            line.Visible = false
        end
        return
    end

    local headPos, headOnScreen = Camera:WorldToViewportPoint(head.Position + Vector3.new(0, 0.5, 0))
    local rootPos, rootOnScreen = Camera:WorldToViewportPoint(hrp.Position - Vector3.new(0, 3, 0))

    if headOnScreen and rootOnScreen and headPos.Z > 0 and rootPos.Z > 0 then
        local height = math.abs(headPos.Y - rootPos.Y)
        local width = height * 0.6

        local x1 = headPos.X - width/2
        local x2 = headPos.X + width/2
        local y1 = headPos.Y
        local y2 = rootPos.Y

        -- Top line
        box.topLeft.From = Vector2.new(x1, y1)
        box.topLeft.To = Vector2.new(x2, y1)

        -- Bottom line
        box.bottomLeft.From = Vector2.new(x1, y2)
        box.bottomLeft.To = Vector2.new(x2, y2)

        -- Left line
        box.topRight.From = Vector2.new(x1, y1)
        box.topRight.To = Vector2.new(x1, y2)

        -- Right line
        box.bottomRight.From = Vector2.new(x2, y1)
        box.bottomRight.To = Vector2.new(x2, y2)

        for _, line in pairs(box) do
            line.Visible = espBoxEnabled and espEnabled
        end
    else
        for _, line in pairs(box) do
            line.Visible = false
        end
    end
end

local function updateESP()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local distance = getPlayerDistance(player)
            if distance <= espMaxDistance then
                local char = player.Character
                local highlight = espHighlights[player]

                if not highlight then
                    highlight = Instance.new("Highlight")
                    highlight.Adornee = char
                    highlight.FillTransparency = 0.5
                    highlight.OutlineTransparency = 0
                    highlight.Parent = char
                    espHighlights[player] = highlight
                end

                -- Cor baseada na distÃ¢ncia
                if distance < 50 then
                    highlight.FillColor = Color3.fromRGB(255, 0, 0)
                    highlight.OutlineColor = Color3.fromRGB(255, 0, 0)
                elseif distance < 200 then
                    highlight.FillColor = Color3.fromRGB(255, 255, 0)
                    highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
                else
                    highlight.FillColor = Color3.fromRGB(0, 255, 0)
                    highlight.OutlineColor = Color3.fromRGB(0, 255, 0)
                end

                highlight.Enabled = espEnabled

                -- Atualizar box ESP
                if not espBoxes[player] then
                    espBoxes[player] = createESPBox(player)
                end
                updateESPBox(player, espBoxes[player])
            else
                -- Remover ESP se muito longe
                if espHighlights[player] then
                    espHighlights[player]:Destroy()
                    espHighlights[player] = nil
                end
                if espBoxes[player] then
                    for _, line in pairs(espBoxes[player]) do
                        line:Remove()
                    end
                    espBoxes[player] = nil
                end
            end
        end
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                      TRACERS MELHORADOS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function updateTracers()
    -- Limpar tracers antigos
    for _, line in pairs(tracerLines) do
        if line then line:Remove() end
    end
    tracerLines = {}

    if not tracersEnabled or not Drawing then return end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local distance = getPlayerDistance(player)
            if distance <= espMaxDistance then
                -- Tracer para o centro do torso do personagem
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local screenPos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
                    if onScreen and screenPos.Z > 0 then
                        pcall(function()
                            local line = Drawing.new("Line")
                            line.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                            line.To = Vector2.new(screenPos.X, screenPos.Y)

                            -- Cor baseada na distÃ¢ncia
                            if distance < 50 then
                                line.Color = Color3.fromRGB(255, 0, 0)
                            elseif distance < 200 then
                                line.Color = Color3.fromRGB(255, 255, 0)
                            else
                                line.Color = Color3.fromRGB(0, 255, 0)
                            end

                            line.Thickness = 2
                            line.Transparency = 1
                            line.Visible = true
                            table.insert(tracerLines, line)
                        end)
                    end
                end
            end
        end
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                      NAMETAGS MELHORADOS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function updateNametags()
    -- Limpar nametags antigos
    for player, gui in pairs(nametagsGuis) do
        if gui then gui:Destroy() end
    end
    nametagsGuis = {}

    if not nametagsEnabled then return end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local distance = getPlayerDistance(player)
            if distance <= espMaxDistance then
                local head = player.Character:FindFirstChild("Head")
                local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                if head and humanoid then
                    local billboard = Instance.new("BillboardGui")
                    billboard.Adornee = head
                    billboard.Size = UDim2.new(0, 200, 0, 80)
                    billboard.StudsOffset = Vector3.new(0, 2, 0)
                    billboard.AlwaysOnTop = true
                    billboard.Parent = head

                    local nameLabel = Instance.new("TextLabel")
                    nameLabel.Size = UDim2.new(1, 0, 0.4, 0)
                    nameLabel.BackgroundTransparency = 1
                    nameLabel.Text = player.Name
                    nameLabel.Font = Enum.Font.GothamBold
                    nameLabel.TextSize = 14
                    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    nameLabel.TextStrokeTransparency = 0.5
                    nameLabel.Parent = billboard

                    local distanceLabel = Instance.new("TextLabel")
                    distanceLabel.Size = UDim2.new(1, 0, 0.3, 0)
                    distanceLabel.Position = UDim2.new(0, 0, 0.4, 0)
                    distanceLabel.BackgroundTransparency = 1
                    distanceLabel.Text = math.floor(distance) .. "m"
                    distanceLabel.Font = Enum.Font.Gotham
                    distanceLabel.TextSize = 12
                    distanceLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
                    distanceLabel.TextStrokeTransparency = 0.5
                    distanceLabel.Parent = billboard

                    local healthLabel = Instance.new("TextLabel")
                    healthLabel.Size = UDim2.new(1, 0, 0.3, 0)
                    healthLabel.Position = UDim2.new(0, 0, 0.7, 0)
                    healthLabel.BackgroundTransparency = 1
                    healthLabel.Text = math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
                    healthLabel.Font = Enum.Font.Gotham
                    healthLabel.TextSize = 12
                    healthLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
                    healthLabel.TextStrokeTransparency = 0.5
                    healthLabel.Parent = billboard

                    nametagsGuis[player] = billboard
                end
            end
        end
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                      VARIÃVEIS DO AIMBOT
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local aimbotEnabled = false
local aimbotFOV = 80
local aimbotTarget = "Head"
local aimbotKey = Enum.UserInputType.MouseButton2
local aimbotKeyName = "BotÃ£o Direito"
local aimbotSmooth = 5
local fovCircle = nil
local waitingForKey = false

-- Criar cÃ­rculo FOV se Drawing estiver disponÃ­vel
if Drawing then
    pcall(function()
        fovCircle = Drawing.new("Circle")
        fovCircle.Thickness = 2
        fovCircle.NumSides = 50
        fovCircle.Filled = false
        fovCircle.Transparency = 1
