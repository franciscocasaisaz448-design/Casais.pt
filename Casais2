godmodeCard[2].Text = "ğŸ›¡ï¸"
godmodeCard[3].Text = "GODMODE"
godmodeCard[4].Text = "ProteÃ§Ã£o total contra danos"

local godmodeToggle = createToggle(godmodeCard, "GODMODE", function(enabled)
    if enabled and LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.MaxHealth = math.huge
            hum.Health = math.huge
        end
    end
end)

